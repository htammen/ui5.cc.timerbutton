{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "rootDir": "./",
    "types": [
      "@openui5/ts-types-esm",
      "@types/jquery",
      "@types/qunit"
    ],
    "paths": {
      "ui5/cc/timerbutton/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "./src/**/*",
    "./test/**/*"
  ]
}
